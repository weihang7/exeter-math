doctype html
html(lang='en')
  head
    meta(charset='utf-8')
    meta(http-equiv='X-UA-Compatible', content='IE=edge')
    meta(name='viewport', content='width=device-width, initial-scale=1')
    title Registered Teams | Exeter Math Club Competition
    link(href='//netdna.bootstrapcdn.com/bootstrap/3.1.1/css/bootstrap.min.css', rel='stylesheet')
    link(href='tablesorter-theme.default.css', rel='stylesheet')
    link(href='main.css', rel='stylesheet')
    script(src='respond.min.js')
    link(rel='shortcut icon', href='favicon.ico')
  body
    include navbar.jade
    div.container
      div.page-header
        h1 Teams
      table#teams_list.table
        thead
          tr
            th Name
            th Id
            th.sorter-digit Team
            th.sorter-digit Guts
            th Sweepstakes
      div.page-header
        h1 Individuals
      table#individuals_list.table
        thead
          tr
            th Team
            th Name
            th Id
            th.sorter-digit Speed
            th.sorter-digit Accuracy
            th.sorter-digit Total
    script(src='//code.jquery.com/jquery-1.11.0.min.js')
    script(src='//netdna.bootstrapcdn.com/bootstrap/3.1.1/js/bootstrap.min.js')
    script(src='jquery.tablesorter.min.js')
    script(src='jquery.tablesorter.widgets.min.js')
    script(src='build/list_scores.js')
